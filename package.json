{
  "name": "ts-practice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "json:server": "json-server --watch ./src/modules/json-server/index.js",
    "json:server:remote": "json-server http://jsonplaceholder.typicode.com/db",
    "start": "nodemon ./src/index.ts",
    "start:js": "nodemon ./src/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "test": "nodemon ./src/test.ts",
    "test-api-with-multiple-requests": "autocannon -c 200 -d 10 http://localhost:3000",
    "forward-port": "ngrok http 3000",
    "pm2": "tsc ./src/index.ts & pm2 start ./src/index.js -i -1",
    "pm2:delete": "pm2 delete all"
  },
  "dependencies": {
    "@types/json-server": "^0.14.4",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "http-errors": "~1.6.3",
    "ilya-korzhavin-github-actions-v2": "^1.2.1",
    "jade": "~1.11.0",
    "json-server": "^0.17.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.6",
    "nodemon": "^2.0.19",
    "pdfmake": "^0.2.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "ts-node": "^10.9.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.6",
    "@types/node-fetch": "^3.0.3",
    "@types/pdfmake": "^0.1.19",
    "@types/yargs": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "express": "^4.17.1",
    "prettier": "^2.4.1",
    "typescript": "^4.8.2"
  }
}
